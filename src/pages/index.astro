<style>
	body {
		background-color: gainsboro;
		background-image: url("../img/ruidoBackground.png");
	}
	main {
		margin: auto;
		padding: 1rem;
		width: 950px;
		max-width: calc(100% - 2rem);
		color: white;
		font-size: 1.1em;
	}
	.botoon {
		width: 100px;
		height: 100px;
		background-color: green;
	}
	.custom {
		background-color: rgb(0, 20, 136);
		border: none;
		height: 2px;
	}
	body.dark .custom {
		background-color: rgb(154, 154, 154);
	}
	.astro-a {
		position: absolute;
		top: -32px;
		left: 50%;
		transform: translatex(-50%);
		width: 220px;
		height: auto;
		z-index: -1;
	}
	.box {
		display: grid;
		grid-template-columns: repeat(3, 1fr);
		grid-template-rows: 1fr;
		grid-column-gap: 15px;
		grid-row-gap: 0px;
		padding: 1em;
		border-radius: 1em;
	}

	.box:hover {
		background-color: rgb(183, 237, 255);
		border-radius: 1em;
		transform: scale(1.1);
		transition: 0.25s ease-in;
	}
	body.dark .box:hover {
		background-color: rgb(0, 10, 32);
	}
	.texto {
		grid-area: 1 / 1 / 2 / 3;
	}
	.img {
